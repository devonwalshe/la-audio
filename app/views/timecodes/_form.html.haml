= form_for(@timecode) do |f|
  - if @timecode.errors.any?
    #error_explanation
      %h2
        = pluralize(@timecode.errors.count, "error")
        prohibited this timecode from being saved:
      %ul
        - @timecode.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :start_time
    %br/
    = f.time_select :start_time
  .field
    = f.label :end_time
    %br/
    = f.time_select :end_time
  .actions
    = f.submit
